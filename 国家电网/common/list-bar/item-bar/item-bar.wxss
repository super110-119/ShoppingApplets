/* common/list-bar/item-bar/item-bar.wxss */
.item{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-evenly;
}
.item-img{
  width: 90rpx;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* background-color: #DFE1E7; */
  border-radius: 50%;
}
.item-img image{
  width: 48rpx;
  height: 48rpx;
}
.active image{
  width: 75rpx;
  height: 48rpx;
}
.titles{
  color: #888;
}

.item_img1{
  background-color: #eeaa00;
}
.item_img2{
  background-color: #66B2FF;
}
.item_img3{
  background-color: #ff6666;
}
.item_img4{
  background-color: #66aa66;
}
.item_img4 image{
  width: 66rpx;
  height: 48rpx;
}
