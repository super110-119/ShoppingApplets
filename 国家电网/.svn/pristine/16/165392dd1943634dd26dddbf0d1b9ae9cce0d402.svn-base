/* common/noDataBig/noDataBig.wxss */
.no_data_container{
  width: 100%;
  overflow: hidden;
  position: fixed;
  height: 100vh;
  top: 0;
  left: 0;
  z-index: -2;
}
.container_box{
  position: absolute;
  height: 30%;
  text-align: center;
  color: #999;
  font-size: 36rpx;
  font-family: "楷体";
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.container_box view{
  margin: 30rpx auto;
}
.box_img{
  width: 285rpx;
  height: 195rpx;
}
.box_img image{
  width: 100%;
  height: 100%;
}
