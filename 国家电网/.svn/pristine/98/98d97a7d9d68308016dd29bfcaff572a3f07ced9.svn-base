/* pages/home/home.wxss */
/* 外层嵌套的盒子 */
.box{
  margin: 0;
  padding: 0;
  width: 100vw;
  background-color: #f2f4f5;
  border-bottom: 20rpx solid #f2f4f5;
}
/* 轮播图 banner */
.banner{
  width: 730rpx;
  margin: 10rpx;
  height: 240rpx;
}

.get_mask{
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 10;
}