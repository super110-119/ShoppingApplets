<!--pages/home/h-compontent/collapse/collapse-item/collapse-item.wxml-->
<view class="item">
  <view class="item-title">{{mes.mesTitle}}</view>
  <view class="item-status">
    <view class="isok" wx-if="{{mes.status === 0}}">已审核通过</view>
    <view class="isno" wx-if="{{mes.status === 1}}">审核未通过</view>
    <view class="isnow" wx-if="{{mes.status === 2}}">正在审查中</view>
  </view>
</view>
