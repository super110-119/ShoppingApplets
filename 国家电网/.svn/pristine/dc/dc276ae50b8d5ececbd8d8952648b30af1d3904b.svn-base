  // 获取用户点击的是哪一个按钮
  listBarClick(e){
    const mes = e.detail
    let i = mes.index
    let t = mes.title
    if(i === 0){
      wx.navigateTo({
        url: '/pages/demand/demand',
      })
    }else if(i === 1){
      wx.navigateTo({
        url: '/pages/reserveitem/createitem/selectitem/selectitem',
      })      
    }else if(i === 2){
      wx.navigateTo({
        url: '/pages/trainingregister/trainingregister',
      })      
    }else if(i === 3){
      wx.reLaunch({
        url: '/pages/trainingclass/trainingclass',
      })      
    }
  },
