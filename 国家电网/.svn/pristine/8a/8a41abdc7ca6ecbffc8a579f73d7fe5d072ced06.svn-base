/* pages/profile/p-compontent/bg/bg.wxss */
.content{
  position: relative;
  height: 320rpx;
}
.content1{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 256rpx;
  background-image: linear-gradient(to bottom, rgb(13, 226, 180), rgba(0, 190, 149, 1));
  z-index: 1;
}
.content2{
  position: absolute;
  height: 128rpx;
  width: 100%;
  background-color: rgba(0, 190, 149, 1);
  border-radius: 50%;
  bottom: 0;
  left: 0;
  right: 0;
}