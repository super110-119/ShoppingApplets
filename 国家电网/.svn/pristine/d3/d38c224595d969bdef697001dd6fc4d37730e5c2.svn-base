/* pages/demand/d-page/particulars/items/items.wxss */
.box{
  margin: 0 24rpx;
  margin-top: 36rpx;
}
.title{
  margin: 16rpx;
  font-size: 30rpx;
  font-weight: 500;
}
.inputs::after{
  content: '';
  display: block;
  width: 100%;
  height: 1px;
  margin-top: 30rpx;
  background-color: #E5E5E5;
}
.inputs label{
  display: flex;
  align-items: center;
  margin: 16rpx 0;
  font-size: 28rpx;
  color: #808080;
}
.inputs label radio{
  margin: 0 10rpx;
}
.checkboxs textarea{
  width: 92%;
  margin: 0 auto;
  height: 180rpx;
  border: 1px solid rgba(187, 187, 187, 1);
  box-sizing: border-box;
}

