/* pages/profile/p-compontent/bg/bg.wxss */
.content{
  position: relative;
  height: 320rpx;
}
.content1{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 256rpx;
  background-image: linear-gradient(to bottom, #b4ccfd, #5d82e2);
  z-index: 1;
}
.content2{
  position: absolute;
  height: 128rpx;
  width: 100%;
  background-color: #5d82e2;
  border-radius: 50%;
  bottom: 0;
  left: 0;
  right: 0;
}