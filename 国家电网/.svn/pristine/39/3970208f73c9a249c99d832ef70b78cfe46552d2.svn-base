/* pages/demand/demand.wxss */
.linear{
  position: fixed;
  width: 100%;
  height: 1rpx;
  top: 0;
  left: 0;
  box-shadow: 0 2rpx 5rpx #666;
}
.nothing{
  font-size: 40rpx;
  text-align: center;
  margin: 30rpx auto;
}

.get_mask{
  width: 100vw;
  height: 100vh;
  background-color: #fff;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 10;
}
