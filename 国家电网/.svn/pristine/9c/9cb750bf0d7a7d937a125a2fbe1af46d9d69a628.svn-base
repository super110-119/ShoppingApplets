.box{
  width: 710rpx;
  height: 550rpx;
  margin: 0 20rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
  padding-bottom: 20rpx;
}
.box-titlt::before{
  content: '|';
  font-size: 32rpx;
  display: inline-block;
  margin: 20rpx 10rpx;
}
.box-titlt-mes{
  margin-left: 10rpx;
  font-size: 30rpx;
}
.box-titlt-more{
  float: right;
  margin: 20rpx;
  font-size: 30rpx;
  color: #5d82e2;
}
.box-list{
  width: 680rpx;
  margin: 0 auto;
  overflow: hidden;
  height: 460rpx;
}

.myClass{
  text-align: center;
  font-size: 42rpx;
  color: #5d82e2;
  margin: 30rpx 0;
}
