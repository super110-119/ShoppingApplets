/* pages/reserveitem/users/consult/consult.wxss */
.box{
  background-color: #EFEFEF;
  height: 100%;
  /* padding-bottom: 80rpx; */
}
.titles::before{
  content: "|";
  display: inline-block;
  margin: 0 10rpx;
}
.titles{
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: 600;
  padding: 22rpx 0;
}
.box-container{
  box-shadow: 0px 2px 6px 0px rgba(0, 0, 0, 0.4);
  font-family: Arial;
  border: 1px solid rgba(187, 187, 187, 1);
  margin: 0 24rpx 30rpx 12rpx;
  color: rgba(93, 90, 90, 1);
  background-color: #fff;
  font-size: 26rpx;
  padding-bottom: 30rpx;
}
.list{
  display: flex;
  margin: -20rpx 0;
}
.item-title{
  margin: 0 30rpx;
  font-weight: 550;
  color: #333;
  white-space: nowrap;
}
.btn{
  margin: 10rpx 26rpx 50rpx 20rpx;
  background-color: #00BE95;
  height: 92rpx;
  line-height: 92rpx;
  border-radius: 10rpx;
  color: #fff;
  font-weight: 550;
  text-align: center;
}

