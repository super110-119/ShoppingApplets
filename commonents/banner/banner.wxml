<!--commonents/banner/banner.wxml-->
<swiper 
      circular 
      autoplay
      indicator-dots
      indicator-active-color="#ff5777"
      interval="3200"
      duration="350">
    <block wx:for="{{banner}}" wx:key="index">
      <swiper-item>
        <image src="{{item.image || item}}"/>
      </swiper-item>
    </block>
  </swiper>
