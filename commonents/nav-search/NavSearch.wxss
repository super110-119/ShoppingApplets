/* commonents/nav-search/NavSearch.wxss */
.nav-search{
  width: 100%;
  height: 60rpx;
  margin: 14rpx 0;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 60rpx;
  font-size: 30rpx;
}
.nav-share{
  width: 100%;
  height: 100%;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.nav-share button{
  width: 120%;
  height: 120%;
  margin: -10%;
  background-color: transparent;
  display: flex;
  justify-content: center;
  align-items: center;
}
