/* pages/mDetail/MDComponent/MDComponent.wxss */
.box{
  width: 100%;
  height: 180rpx;
  position: relative;
  margin-bottom: 15rpx;
  border-bottom: 1rpx solid #ccc;
}
.box>view{
  position: absolute;
}

.title{
  top: 20rpx;
  left: 20rpx;
}
.time{
  bottom: 30rpx;
  left: 20rpx;
}
.money{
  top: 20rpx;
  right: 20rpx;
}
.balance{
  bottom: 30rpx;
  right: 20rpx;
}
.small{
  font-size: 26rpx;
  color: #666;
}
.big{
  font-weight: 550;
}
.add{
  color: #3F87EE;
}
.reduce{
  color: #c44;
}
