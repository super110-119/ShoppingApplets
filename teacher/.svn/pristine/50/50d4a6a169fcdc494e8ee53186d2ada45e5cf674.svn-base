/* pages/myClass/info/info.wxss */
.box{
  width: 680rpx;
  height: 680rpx;
  margin: 50rpx auto;
  display: flex;
  flex-wrap: wrap;
  box-shadow: 0px 0px 3px 1px #c4c4c4;
}
.box>view{
  width: 50%;
  box-sizing: border-box;
  border: 1rpx solid #ccc;
}
.boxs{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.box1{
  color: #3F87EE;
  font-size: 42rpx;
  font-weight: 550;
}
