<!--pages/myClass/classComponents/classItem/classItem.wxml-->
<view class="box">
  <view class="image">
    <image src="{{message.imgUrl}}" mode="aspectFill"/>
  </view>
  <view class="title line_clamp2">{{message.titles}}</view>
  <view class="time">开始时间：{{message.beginTime}}</view>
  <view class="btns">
    <view class="btn btn1" bindtap="infoClick">信息</view>
    <view class="btn btn2" bindtap="successClick">笔记</view>
    <view class="btn btn3" bindtap="warningClick">提问</view>
  </view>
</view>
